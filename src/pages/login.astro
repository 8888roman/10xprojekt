---
import Layout from "../layouts/Layout.astro";
import { LoginForm } from "../components/auth/LoginForm";

const {
  data: { user },
} = await Astro.locals.supabase.auth.getUser();

if (user) {
  return Astro.redirect("/generate");
}
---

<Layout title="Logowanie">
  <LoginForm client:load />
</Layout>
